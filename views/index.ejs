<% include partials/header %>

<h1>Posts</h1>


<% posts.forEach(function(post){ %>
  <div>
      <h3><%= post.title %></h3>
      <img src="<%= post.image %>" alt="header image">
      <p><%= post.date %></p>
      <a href="<%= "/posts/" + post._id %>">More</a>
  </div>
<% }) %>



<% include partials/footer %>